<div class="filter">
    	<div class="filter-head">
		    <span class="title">
		        <span class="g-icon-h">
		        	<span class="strelka"></span>
		        </span><b class="g-pseudo-h"><%= name %></b>
		        <span class="b-catalog-filter-item-help js-construct"></span>
		    </span>
		</div>
		<div class="filter-body">
				<% array.each do |a| %>
				<% checked = false %>
					<% checked = params[:property].include?("#{a.id}") if params[:property]  %>
	                <div>
	                	<%= check_box_tag "property[#{a.id}]", a.id, false, checked: checked || false %>
	                	<%= label_tag "property[#{a.id}]", a.value  %>
	                </div>
	            <% end %>
		</div>
	</div>