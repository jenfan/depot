<table class="table table-hover table-condensed">
	<tr>
		<th>ID</th>
		<th>Name</th>
		<th>save</th>
	</tr>
	<%= render partial: 'admin/view/interest', collection: @interests, as: :interest  %>
</table>

<%= form_for :interest, url:{action: 'interest_create'} do |i| %>
	<%= i.label :name %><%= i.text_field :name %>
	<%= i.submit %>
<% end %>