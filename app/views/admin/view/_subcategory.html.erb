<tr>
     <%= form_for subcategory, url: {action: "subcategory_update"} do |f| %>
        
        <td>
            <%= f.label :id %><br /><%= f.text_field :id, size: 4 %>
        </td>
        <td>
            <%= f.select :category_id, @categories.map { |city| [city.name, city.id] }, prompt: '...' %>
         </td>
        <td>
            <%= f.label :name %><br /><%= f.text_field :name, size: 40 %>
        </td>
        <td>
            <%= f.label :url_name %><br /><%= f.text_field :url_name, size: 40 %>
        </td>
        <td><%= f.submit "save",  class:"btn btn-primary" %>
        <%= f.submit "x",  class:"btn btn-danger" %></td>
         
     <% end %>
</tr>
   